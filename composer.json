{
    "name": "reedware/laravel-dump-to-server",
    "description": "Adds the ability to dump only when the dump server is online.",
    "keywords": ["laravel", "dump", "server"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tyler Reed",
            "email": "tylernathanreed@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "illuminate/container": "^6.0|^7.0|^8.0",
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^8.5|^9.5"
    },
    "autoload": {
        "psr-4": {
            "Reedware\\LaravelDumpToServer\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Reedware\\LaravelDumpToServer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Reedware\\LaravelDumpToServer\\ServiceProvider"
            ]
        }
    }
}
